<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="custom_apicom_view_stock_warehouse_form" model="ir.ui.view">
            <field name="name">custom.apicom.stock.warehouse</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='wh_output_stock_loc_id']" position="after">
                    <field name="rental_stock_loc_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>