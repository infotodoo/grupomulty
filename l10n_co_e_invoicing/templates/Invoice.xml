					NitFacturador={{ProviderID}}
					NitAdquiriente={{NitAdquiriente}}
					FechaFactura={{IssueDate}}
					ValorTotalFactura={{PayableAmount}}
					CUFE={{UUID}}
					URL={{QRCodeURL}}</sts:QRCode>
				</sts:DianExtensions>
			</ext:ExtensionContent>
		</ext:UBLExtension>
		<ext:UBLExtension>
