                            <xades:IssuerSerial>
                                <ds:X509IssuerName>{{X509IssuerName}}</ds:X509IssuerName>
                                <ds:X509SerialNumber>{{X509SerialNumber}}</ds:X509SerialNumber>
                            </xades:IssuerSerial>
                        </xades:Cert>
